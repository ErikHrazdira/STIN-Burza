<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="Code Coverage" uri="clr://Microsoft/CodeCoverage/1.0" assemblyQualifiedName="Microsoft.VisualStudio.Coverage.Collector.CoverageDataCollector, Microsoft.VisualStudio.Coverage.Collector, Version=16.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
        <Configuration>
          <CodeCoverage>
            <ModulePaths>
              <Exclude>
                <ModulePath>.*\\Program\.cs$</ModulePath>
                <ModulePath>.*\\Views\\.*\.cshtml$</ModulePath>
              </Exclude>
            </ModulePaths>
          </CodeCoverage>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>