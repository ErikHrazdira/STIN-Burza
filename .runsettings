<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage" uri="datacollector://Microsoft/CodeCoverage/1.0" assemblyQualifiedName="Microsoft.VisualStudio.CodeCoverage.CoverageDataCollector, Microsoft.VisualStudio.CodeCoverage, Version=16.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
        <Configuration>
          <Format>cobertura</Format>
          <CoverageFileName>coverage.cobertura.xml</CoverageFileName>
          <UseSourceLink>true</UseSourceLink>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
  <CollectCoverage>true</CollectCoverage>
  <CoverageOutput>TestResults/Coverage</CoverageOutput>
  <CoverageOutputFormat>cobertura</CoverageOutputFormat>
</RunSettings>